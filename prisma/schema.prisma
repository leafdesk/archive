generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Member {
  id          Int      @id @default(autoincrement())
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
  name        String
  sex         String
  department  String
  address     String?
  school      String?
  major       String?
  team        String?
  phoneNumber Int?     @unique
  email       String?  @unique
}
