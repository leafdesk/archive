generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Member {
  id          Int       @id @default(autoincrement())
  name        String
  gender      Int
  birth_date  DateTime?
  nation      String?
  create_date DateTime  @default(now())
  update_date DateTime
}
